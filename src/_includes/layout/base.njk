<!DOCTYPE html>
<html lang="{{ global_index.lang }}">
<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta name="google-site-verification" content="{{ global_index.search_console }}" />
  <title>{{ metaTitle }}</title>
  <meta name="robots" content="{{ metaRobots }}" />
  <meta name="description" content="{{ metaDescription or eleventyComputed.metaDescription or metadata.description }}" />
  <meta name="keywords" content="{{ metaKeywords or eleventyComputed.metaKeywords }}" />
  <link rel="manifest" href="/site.webmanifest">
  <meta property="og:locale" content="es_ES" />
  <meta property="og:site_name" content="Check - {{ metaKeywords or eleventyComputed.metaKeywords }}" />
  <meta property="og:type" content="website" />
  <meta property="og:title" content="{{ metaTitle }}" />
  <meta property="og:description" content="{{ metaDescription or eleventyComputed.metaDescription or metadata.description }}" />
  <meta property="og:url" content="{{ global_index.url }}{{ page.url | url }}" />
  <meta property="og:image" content="{{ global_index.url }}/assets/static/images/2.webp" />
  <meta name="twitter:card" content="summary_large_image" />
  <meta name="twitter:title" content="{{ metaTitle }}" />
  <meta name="twitter:description" content="{{ metaDescription or eleventyComputed.metaDescription or metadata.description }}" />
  <meta name="twitter:image" content="{{ global_index.url }}/assets/static/favicons/favicon-96x96.png" />
  <link rel="icon" type="image/png" href="{{ global_index.url }}/assets/static/favicons/favicon-32x32.png" />
  <!-- Estilos CSS -->
  <style>
    :root { 
      --c-accent: {{ global_index.c_accent }}; 
      --c-primary: {{ global_index.c_primary }};
      --c-secondary: {{ global_index.c_secondary }}; 
      --bg-dark: {{ global_index.bg_dark }}; 
      --c-bg-dark: {{ global_index.c_bg_dark }}; 
      --bg-light: {{ global_index.bg_light }};
      --bg-white: {{ global_index.bg_white }};
      --c-bg-light: {{ global_index.c_bg_light }};
      --btn-radius: {{ global_index.btn_radius }}; 
      --padding-global: {{ global_index.padding_global }};
    }
  </style>
  <link rel="stylesheet" href="/assets/css/style.css" />
  <link rel="canonical" href="{{ global_index.url }}{{ page.url | url }}" />
  {% if global_index.tag_manager %}
  <script>
    (function (w, d, s, l, i) {
      w[l] = w[l] || []; w[l].push({
        'gtm.start': new Date().getTime(), event: 'gtm.js'
      }); 
      var f = d.getElementsByTagName(s)[0], j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; 
      j.async = true; 
      j.src = 'https://www.googletagmanager.com/gtm.js?id=' + i + dl; 
      f.parentNode.insertBefore(j, f);
    })(window, document, 'script', 'dataLayer', '{{ global_index.tag_manager }}');
  </script>
  {% endif %}
</head>
<body data-barba="wrapper" class="contain {{ colorHeader }} {% if page.url == '/' %}home{% endif %}">
  <!-- Google Tag Manager (noscript) -->
  {% if global_index.tag_manager %}
  <noscript>
    <iframe title="googletagmanager" src="https://www.googletagmanager.com/ns.html?id={{ global_index.tag_manager }}" height="0" width="0" style="display:none;visibility:hidden"></iframe>
  </noscript>
  {% endif %}


  <header>
      <!-- Header fuera del contenedor de Barba.js 
<div class="current-section">

</div>

-->
    
    {% include 'components/header.njk' %}
  </header>

  <div id="barba-wrapper">
    <div class="barba-container" data-barba="container" data-barba-namespace="home">
   
 
      {{ content | safe }}

    </div>  </div>

  <div class="transition-overlay"></div>

  <!-- Scripts -->
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>


  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.5/gsap.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/ukiyojs@4.0.3/dist/ukiyo.umd.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/@studio-freight/lenis@latest/dist/lenis.min.js"></script>
  <script src="https://unpkg.com/gsap@3/dist/SplitText.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/@barba/core"></script>
  <script type="module" src="/assets/js/appear-text.js"></script>

  <script type="module" src="/assets/js/index.js"></script>
  <script type="module" src="/assets/js/scroll.js"></script>
  <script type="module" src="/assets/js/inview.js"></script>
  <script type="module" src="/assets/js/barba.js"></script>


